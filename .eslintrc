{
	"rules": {
		"strict": 0,
		"new-cap": 0,
		"babel/object-curly-spacing": 0,
		"babel/arrow-parens": 0,
		"react/no-multi-comp": 0,
		"block-scoped-var": 0,
		"padded-blocks": 0,
		"comma-dangle": 0,
		"indent": [2, "tab", {
			"SwitchCase": 1
		}],
		"no-console": 0,
		"no-alert": 0,
		"no-unused-vars": 1,
		"spaced-comment": 0,
		"no-nested-ternary": 0
	},
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended"
}
